Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Fierz", "::", "usage"}], "=", 
    "\"\<Fierz[expr_,indices_List,opts___Rule] will try to Fierz-contract the \
indices given in indices (which has to be a list) assuming the generators to \
be t[a_] and the dimension of the fundamental representation to be Nc. These \
should be turned to be options of course.\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FierzP", "::", "usage"}], "=", 
    "\"\<FierzP is an option to Fierz. By default set to FierzP. FierzP[i_] \
in a product (FierzProduct) projects onto an external index i. As such it \
makes only sense to have it show up as a first or last element of the \
product, \>\""}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Fierzt", "::", "usage"}], "=", 
    "\"\<Fierzt is an option to Fierz. It stands for the generators of SU(Nc) \
to be used. By default set to Fierzt.\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FierzN", "::", "usage"}], "=", 
    "\"\<FierzN is an option to Fierz. It stands N in SU(N) to be used. By \
default set to FierzN.\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FierzProduct", "::", "usage"}], "=", 
    "\"\<FierzProduct is an option for Fierz. By default it is set to \
FierzProduct, but you better intruduce your own.\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FierzTrace", "::", "usage"}], "=", 
    "\"\<FierzTrace is an option for Fierz, by default set to FierzTrace, but \
you better intruduce your own.\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"FierzDelta", "::", "usage"}], "=", 
    "\"\<FierzDelta is an option for Fierz, by default set to Global`delta.\>\
\""}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
